@games.map do |game|
  {
    id: game.id,
    player_id: game.player_id,
    field_id: game.field_id,
    date_time: game.date_time,
    attending: game.players_attending.map do |attendee|
      {
        player_attending: attendee.player_id, # I only want to show player_id if attending: true
      }
    end,
  }
end
